{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "albertcht/invisible-recaptcha": "^1.8",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-snappy": "^0.4.1",
        "cviebrock/eloquent-sluggable": "^4.5",
        "fideloper/proxy": "^4.0",
        "h4cc/wkhtmltoimage-i386": "^0.12.4",
        "h4cc/wkhtmltopdf-i386": "^0.12.4",
        "jenssegers/date": "^3.3",
        "jrean/laravel-user-verification": "dev-master",
        "kwn/number-to-words": "^1.5",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "^1.0",
        "laraveles/spanish": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^0.14.1",
        "spatie/laravel-activitylog": "^2.5",
        "spatie/laravel-backup": "^5.5",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
        "zizaco/entrust": "^1.9"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
